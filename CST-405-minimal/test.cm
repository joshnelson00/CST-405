int addNums(int a, int b) {
    int result;
    result = a + b;
    return result;
}

int multNums(int x, int y) {
    int product;
    product = x * y;
    return product;
}

int computeVal(int n) {
    int temp1;
    int answer;
    temp1 = addNums(n, 8);
    answer = multNums(temp1, 3);
    return answer;
}

int sumArrayTwo(int arr[], int sz) {
    int sum;
    sum = arr[0] + arr[1];
    return sum;
}

void main() {
    int nums[3];
    
    nums[0] = 100;
    nums[1] = 50;
    
